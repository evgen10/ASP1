
@{
    ViewBag.Title = "Index";
}

<h2>Students</h2>
<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Bithdate</th>
        <th>Age</th>
    </tr>
    @foreach (var item in ViewBag.Students)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Birthdate.ToLongDateString()</td>
            <td>@item.Age</td>
    </tr>
    }
</table>
