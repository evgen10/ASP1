
@{
    ViewBag.Title = "Index";
}

<h2>Awards</h2>
<table>
    <tr>
        <th>Title</th>
        <th>Description</th>      
    </tr>
    @foreach (var item in ViewBag.Awards)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Title</td>    
            <td>@item.Description</td> 
        </tr>
    }
</table>

